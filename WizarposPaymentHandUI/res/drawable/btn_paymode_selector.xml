    	</shape>    
    </item>
    <item>
    	<shape>
    	    <corners android:radius="6dp"/>
    	    <stroke android:width="1dp"
    	        	android:color="#d8d8d8"/>
    	    <solid android:color="#ffffff"/>
    	</shape> 
    </item>   

</selector>